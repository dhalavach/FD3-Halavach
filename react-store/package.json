{
  "name": "react-store",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "sass src/sass/styles.scss:src/styles.css && vite",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "sass src/sass/styles.scss:src/styles.css && tsc && vite build",
    "build:experimental": "npm run build:types && vite build && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel dist --out-dir dist --extensions \".js,.jsx\" --source-maps inline",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "axios": "^1.3.4",
    "react": "^18.2.0",
    "react-awesome-reveal": "^4.2.3",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "sass": "^1.60.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-modal": "^3.13.1",
    "@vitejs/plugin-react": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-babel": "^1.1.3"
  }
}
